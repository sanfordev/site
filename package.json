{
  "name": "develop",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.2.4"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint --fix",
    "format": "prettier --write",
    "format:check": "prettier --check",
    "analyze": "ANALYZE=true NODE_OPTIONS='--max-old-space-size=4096' next build",
    "postbuild": "next-on-netlify",
    "prepare": "node .github/scripts/prepare-hooks.ts",
    "dev:netlify": "netlify dev",
    "clean": "rm -rf .next out .netlify/plugins/node_modules node_modules/.cache",
    "reset": "npm run clean && npm install",
    "ci": "npm run lint && npm run type-check && npm run test:unit:ci",
    "test": "npm run test:unit && npm run test:e2e",
    "test:unit": "jest",
    "test:unit:watch": "jest --watch",
    "test:unit:coverage": "jest --coverage",
    "test:unit:debug": "jest --detectOpenHandles --forceExit",
    "test:unit:ci": "jest --ci --passWithNoTests",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:report": "playwright show-report",
    "prebuild": "npm run clean",
    "pre-commit": "lint-staged",
    "type-check": "tsc --noEmit",
    "netlify:build": "next build",
    "verify:files": "ls -la .next/"
  },
  "dependencies": {
    "@axe-core/react": "^4.10.1",
    "@formspree/react": "^3.0.0",
    "@hookform/resolvers": "^5.0.0",
    "@netlify/plugin-nextjs": "^5.10.4",
    "@next/eslint-plugin-next": "^15.3.0",
    "@radix-ui/react-slot": "^1.1.2",
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/postcss": "^4.1.3",
    "@tailwindcss/typography": "^0.5.10",
    "@types/node": "^20.17.30",
    "@types/testing-library__jest-dom": "^5.14.9",
    "@types/testing-library__react": "^10.0.1",
    "@vercel/analytics": "^1.5.0",
    "autoprefixer": "^10.4.21",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "critters": "^0.0.23",
    "date-fns": "^4.1.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "gray-matter": "^4.0.3",
    "lucide-react": "^0.487.0",
    "markdown-to-jsx": "^7.7.4",
    "next": "14.2.27",
    "next-cloudinary": "^6.16.0",
    "next-on-netlify": "^3.0.1",
    "next-themes": "^0.4.6",
    "playwright": "^1.51.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.55.0",
    "sharp": "^0.33.5",
    "sonner": "^2.0.3",
    "tailwind-merge": "^3.2.0",
    "tailwindcss": "^3.4.17",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5.8.3",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@emotion/jest": "^11.13.0",
    "@eslint/eslintrc": "^3",
    "@formspree/core": "^4.0.0",
    "@netlify/plugin-nextjs": "^5.10.5",
    "@next/bundle-analyzer": "^14.2.28",
    "@playwright/test": "^1.51.1",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@types/jest": "^29.5.14",
    "@types/react": "18.2.0",
    "@types/react-dom": "18.2.0",
    "eslint": "^8.57.0",
    "eslint-config-next": "14.2.26",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-prettier": "^5.2.5",
    "jest": "^29.2.0",
    "jest-environment-jsdom": "^29.2.0",
    "lint-staged": "^15.5.0",
    "next-pwa": "^5.6.0",
    "postcss": "^8.5.3",
    "prettier": "^3.5.3",
    "ts-node": "^10.9.2"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,md,mdx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,yml,yaml}": [
      "prettier --write"
    ]
  }
}
