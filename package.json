{
  "name": "develop",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "NEXT_TELEMETRY_DISABLED=1 next build",
    "start": "next start",
    "lint": "next lint --fix",
    "format": "prettier --write",
    "format:check": "prettier --check",
    "analyze": "ANALYZE=true NODE_OPTIONS='--max-old-space-size=4096' next build",
    "netlify-build": "npm run build",
    "postinstall": "node -e \"process.env.NETLIFY ? require('child_process').execSync('npm i sharp@latest') : console.log('Skipping sharp installation')\"",
    "postbuild": "cp -r .next/static .next/standalone/.next/ && cp -r public .next/standalone/",
    "prepare": "node .github/scripts/prepare-hooks.js",
    "dev:netlify": "netlify dev --target-port 3000",
    "clean": "rm -rf .next out node_modules/.cache",
    "prebuild": "npm run clean",
    "pre-commit": "lint-staged",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.0.0",
    "@netlify/plugin-nextjs": "^5.10.3",
    "@radix-ui/react-slot": "^1.1.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "critters": "^0.0.23",
    "date-fns": "^4.1.0",
    "framer-motion": "^12.6.2",
    "glob": "^10.4.5",
    "gray-matter": "^4.0.3",
    "lucide-react": "^0.487.0",
    "markdown-to-jsx": "^7.7.4",
    "next": "14.2.27",
    "next-themes": "^0.4.6",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.55.0",
    "react-intersection-observer": "^9.16.0",
    "sharp": "^0.34.1",
    "sonner": "^2.0.3",
    "sugar-high": "^0.9.3",
    "tailwind-merge": "^3.0.2",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@next/bundle-analyzer": "^14.2.27",
    "@next/swc-darwin-arm64": "^14.2.27",
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/typography": "^0.5.10",
    "@types/node": "^20.17.30",
    "@types/react": "18.2.0",
    "@types/react-dom": "18.2.0",
    "autoprefixer": "^10.4.17",
    "eslint": "^8.57.0",
    "eslint-config-next": "14.2.26",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-prettier": "^5.2.5",
    "lint-staged": "^15.5.0",
    "next-pwa": "^5.6.0",
    "postcss": "^8.4.35",
    "prettier": "^3.5.3",
    "tailwindcss": "^3.4.1",
    "tailwindcss-animate": "^1.0.7",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.3"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,yml,yaml}": [
      "prettier --write"
    ]
  },
  "os": [
    "darwin"
  ],
  "cpu": [
    "arm64"
  ],
  "optionalDependencies": {
    "@next/swc-darwin-arm64": "14.2.26"
  }
}
